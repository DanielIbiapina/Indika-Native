{
  "expo": {
    "name": "indika-native",
    "slug": "indika-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/indikalogo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.indika.app",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/indikalogo.jpg",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "indika",
              "host": "payment",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.indika.app",
      "versionCode": 1,
      "config": {
        "googleSignIn": {
          "apiKey": "86807468256-khufgr65rjpdhsa5hvooesnm07shfulk.apps.googleusercontent.com"
        }
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "indika",
    "extra": {
      "eas": {
        "projectId": "0cebe227-ea81-4706-9cb8-7884a86a3077"
      }
    },
    "plugins": [
      "@react-native-google-signin/google-signin"
    ]
  }
}
